{
	"extends": [
		"airbnb-base",
		"prettier"
	],
	"plugins": [
		"prettier"
	],
	"env": {
		"browser": true,
		"commonjs": true,
		"es6": true
	},
	"rules": {
		"prettier/prettier": ["error"],
		"class-methods-use-this": 0,
		"import/no-unresolved": 0,
		"linebreak-style": 0,
		"no-param-reassign": 0
	}
}
